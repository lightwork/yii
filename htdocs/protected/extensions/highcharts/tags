defaultOptions	HighchartsWidget.php	/^		$defaultOptions = array('chart' => array('renderTo' => $id), 'exporting' => array('enabled' => true));$/;"	v
htmlOptions	HighchartsWidget.php	/^	public $htmlOptions = array();$/;"	v
id	HighchartsWidget.php	/^		$id = $this->getId();$/;"	v
jsOptions	HighchartsWidget.php	/^		$jsOptions = CJavaScript::encode($this->options);$/;"	v
options	HighchartsWidget.php	/^		$this->options = CMap::mergeArray($defaultOptions, $this->options);$/;"	v
options	HighchartsWidget.php	/^	public $options = array();$/;"	v
position	HighchartsWidget.php	/^		$this->position = Yii::app()->getClientScript()->coreScriptPosition;$/;"	v
position	HighchartsWidget.php	/^	public $position = CClientScript::POS_END; \/\/ script position$/;"	v
