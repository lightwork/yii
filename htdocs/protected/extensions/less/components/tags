LessCompiler	LessCompiler.php	/^class LessCompiler extends CApplicationComponent$/;"	c
_parser	LessCompiler.php	/^		$this->_parser = new \\Less\\Parser($env);$/;"	v
_parser	LessCompiler.php	/^	protected $_parser;$/;"	v
basePath	LessCompiler.php	/^			$this->basePath = Yii::getPathOfAlias('webroot');$/;"	v
basePath	LessCompiler.php	/^	public $basePath;$/;"	v
compile	LessCompiler.php	/^	public function compile()$/;"	f
compiled	LessCompiler.php	/^			$compiled = $this->getLastModified($destination);$/;"	v
compress	LessCompiler.php	/^	public $compress = false;$/;"	v
css	LessCompiler.php	/^			$css = $this->_parser->parseFile($filename);$/;"	v
debug	LessCompiler.php	/^	public $debug = false;$/;"	v
destination	LessCompiler.php	/^			$destination = realpath($destination);$/;"	v
dir	LessCompiler.php	/^				$dir = dir($path);$/;"	v
dirs	LessCompiler.php	/^		$dirs = array();$/;"	v
entry	LessCompiler.php	/^					$entry = $path.'\/'.$entry;$/;"	v
env	LessCompiler.php	/^		$env = new \\Less\\Environment();$/;"	v
forceCompile	LessCompiler.php	/^	public $forceCompile = false;$/;"	v
fromPath	LessCompiler.php	/^			$fromPath = $this->basePath.'\/'.$lessPath;$/;"	v
getLastModified	LessCompiler.php	/^	protected function getLastModified($path)$/;"	f
hasChanges	LessCompiler.php	/^	protected function hasChanges()$/;"	f
init	LessCompiler.php	/^	public function init()$/;"	f
lastCompiled	LessCompiler.php	/^				$lastCompiled = $compiled;$/;"	v
lastModified	LessCompiler.php	/^						$lastModified = $modified;$/;"	v
lastModified	LessCompiler.php	/^				$lastModified = $modified;$/;"	v
lastModified	LessCompiler.php	/^				$lastModified = null;$/;"	v
modified	LessCompiler.php	/^						$modified = $stat['mtime'];$/;"	v
modified	LessCompiler.php	/^						$modified = $this->getLastModified($entry);$/;"	v
modified	LessCompiler.php	/^			$modified = $this->getLastModified($dir);$/;"	v
parse	LessCompiler.php	/^	public function parse($filename)$/;"	f
paths	LessCompiler.php	/^	public $paths = array();$/;"	v
source	LessCompiler.php	/^			$source = realpath($source);$/;"	v
stat	LessCompiler.php	/^						$stat = stat($entry);$/;"	v
stat	LessCompiler.php	/^				$stat = stat($path);$/;"	v
